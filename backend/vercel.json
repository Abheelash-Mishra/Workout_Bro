{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://workout-bro-api.vercel.app"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}